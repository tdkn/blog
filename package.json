{
  "name": "blog",
  "version": "0.1.0",
  "packageManager": "yarn@4.1.0",
  "private": true,
  "author": "Shun Tedokon <me@tdkn.dev> (https://tdkn.dev)",
  "bugs": {
    "url": "https://github.com/tdkn/blog/issues"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "yarn build && yarn start",
    "analyze": "ANALYZE=true yarn build",
    "lint": "npm-run-all --aggregate-output -l -p 'lint:*'",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "next lint --dir .",
    "lint:stylelint": "stylelint -f verbose 'src/**/*.css' --max-warnings=0",
    "lint:types": "tsc --pretty --noEmit",
    "fix": "npm-run-all -l -c --sequential 'fix:*'",
    "fix:eslint": "next lint --fix --dir .",
    "fix:stylelint": "stylelint './src/**/*.css' -f verbose --fix",
    "fix:prettier": "prettier --write .",
    "test": "vitest",
    "postinstall": "is-ci || husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "@fortawesome/free-brands-svg-icons": "6.5.1",
    "@fortawesome/free-regular-svg-icons": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@next/bundle-analyzer": "14.1.1",
    "@vercel/og": "0.6.2",
    "clsx": "2.1.0",
    "contentlayer": "0.3.4",
    "date-fns": "3.3.1",
    "framer-motion": "11.0.8",
    "next": "14.1.1",
    "next-contentlayer": "0.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-pretty-code": "0.13.0",
    "shiki": "0.14.7",
    "tailwind-merge": "2.2.1"
  },
  "devDependencies": {
    "@testing-library/react": "14.2.1",
    "@types/react": "18.2.58",
    "@types/react-dom": "18.2.19",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.18",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-perfectionist": "2.5.0",
    "husky": "9.0.11",
    "is-ci": "3.0.1",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.2",
    "npm-run-all2": "6.1.2",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.11",
    "remark-emoji": "4.0.1",
    "remark-images": "4.0.0",
    "stylelint": "16.2.1",
    "stylelint-config-standard": "35.0.0",
    "stylelint-config-tailwindcss": "0.0.7",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "vercel": "33.5.3",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.3.1"
  },
  "volta": {
    "node": "20.11.1"
  }
}
