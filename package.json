{
  "name": "blog",
  "version": "0.1.0",
  "packageManager": "yarn@3.4.1",
  "private": true,
  "author": "Shun Tedokon <me@tdkn.dev> (https://tdkn.dev)",
  "bugs": {
    "url": "https://github.com/tdkn/blog/issues"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "yarn build && yarn start",
    "analyze": "ANALYZE=true yarn build",
    "lint": "npm-run-all --aggregate-output -l -p 'lint:*'",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "next lint",
    "lint:types": "tsc --pretty --noEmit",
    "fix": "npm-run-all -l -c --sequential 'fix:*'",
    "fix:eslint": "next lint --fix",
    "fix:stylelint": "stylelint './src/**/*.css' -f verbose --fix",
    "fix:prettier": "prettier --write .",
    "test": "vitest",
    "postinstall": "is-ci || husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.3.0",
    "@fortawesome/free-brands-svg-icons": "6.3.0",
    "@fortawesome/free-regular-svg-icons": "6.3.0",
    "@fortawesome/free-solid-svg-icons": "6.3.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@next/bundle-analyzer": "13.1.6",
    "@vercel/og": "^0.1.0",
    "clsx": "^1.2.1",
    "contentlayer": "^0.3.0",
    "date-fns": "2.29.3",
    "framer-motion": "8.5.5",
    "next": "13.1.6",
    "next-contentlayer": "^0.3.0",
    "next-seo": "5.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-pretty-code": "^0.9.2",
    "shiki": "^0.14.0",
    "tailwind-merge": "^1.8.1"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-react": "^3.0.1",
    "autoprefixer": "^10.4.13",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.5",
    "eslint-config-prettier": "8.6.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jsdom": "^21.1.0",
    "lint-staged": "13.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "remark-emoji": "3.1.1",
    "remark-images": "3.1.0",
    "stylelint": "^15.0.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^30.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.5",
    "vercel": "^28.13.2",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json,md,mdx}": "prettier --write"
  },
  "volta": {
    "node": "18.14.2"
  }
}
