{
  "name": "blog",
  "version": "0.1.0",
  "packageManager": "yarn@3.6.3",
  "private": true,
  "author": "Shun Tedokon <me@tdkn.dev> (https://tdkn.dev)",
  "bugs": {
    "url": "https://github.com/tdkn/blog/issues"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "yarn build && yarn start",
    "analyze": "ANALYZE=true yarn build",
    "lint": "npm-run-all --aggregate-output -l -p 'lint:*'",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "next lint",
    "lint:types": "tsc --pretty --noEmit",
    "fix": "npm-run-all -l -c --sequential 'fix:*'",
    "fix:eslint": "next lint --fix",
    "fix:stylelint": "stylelint './src/**/*.css' -f verbose --fix",
    "fix:prettier": "prettier --write .",
    "test": "vitest",
    "postinstall": "is-ci || husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.4.2",
    "@fortawesome/free-brands-svg-icons": "6.4.2",
    "@fortawesome/free-regular-svg-icons": "6.4.2",
    "@fortawesome/free-solid-svg-icons": "6.4.2",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@next/bundle-analyzer": "13.4.19",
    "@vercel/og": "^0.5.0",
    "clsx": "^2.0.0",
    "contentlayer": "^0.3.0",
    "date-fns": "2.30.0",
    "framer-motion": "10.16.4",
    "next": "13.4.19",
    "next-contentlayer": "^0.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-pretty-code": "^0.10.0",
    "shiki": "^0.14.1",
    "tailwind-merge": "^1.10.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jsdom": "^22.0.0",
    "lint-staged": "14.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "remark-emoji": "4.0.0",
    "remark-images": "3.1.0",
    "stylelint": "^15.2.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.2.7",
    "typescript": "5.2.2",
    "vercel": "^32.0.0",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.34.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json,md,mdx}": "prettier --write"
  },
  "volta": {
    "node": "18.17.1"
  }
}
