{
  "name": "blog",
  "version": "0.1.0",
  "packageManager": "yarn@4.0.2",
  "private": true,
  "author": "Shun Tedokon <me@tdkn.dev> (https://tdkn.dev)",
  "bugs": {
    "url": "https://github.com/tdkn/blog/issues"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "yarn build && yarn start",
    "analyze": "ANALYZE=true yarn build",
    "lint": "npm-run-all --aggregate-output -l -p 'lint:*'",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "next lint --dir .",
    "lint:stylelint": "stylelint -f verbose 'src/**/*.css' --max-warnings=0",
    "lint:types": "tsc --pretty --noEmit",
    "fix": "npm-run-all -l -c --sequential 'fix:*'",
    "fix:eslint": "next lint --fix --dir .",
    "fix:stylelint": "stylelint './src/**/*.css' -f verbose --fix",
    "fix:prettier": "prettier --write .",
    "test": "vitest",
    "postinstall": "is-ci || husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "@fortawesome/free-brands-svg-icons": "6.5.1",
    "@fortawesome/free-regular-svg-icons": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@next/bundle-analyzer": "14.0.4",
    "@vercel/og": "0.6.1",
    "clsx": "2.0.0",
    "contentlayer": "0.3.4",
    "date-fns": "3.0.6",
    "framer-motion": "10.16.16",
    "next": "14.0.4",
    "next-contentlayer": "0.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-pretty-code": "0.10.2",
    "shiki": "0.14.6",
    "tailwind-merge": "2.1.0"
  },
  "devDependencies": {
    "@testing-library/react": "14.1.2",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.16",
    "eslint": "8.55.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-perfectionist": "2.5.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jsdom": "23.0.1",
    "lint-staged": "15.2.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.32",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "0.5.9",
    "remark-emoji": "4.0.0",
    "remark-images": "4.0.0",
    "stylelint": "16.0.2",
    "stylelint-config-standard": "35.0.0",
    "stylelint-config-tailwindcss": "0.0.7",
    "tailwindcss": "3.3.6",
    "typescript": "5.3.3",
    "vercel": "33.0.1",
    "vite-tsconfig-paths": "4.2.2",
    "vitest": "1.0.4"
  },
  "volta": {
    "node": "20.10.0"
  }
}
